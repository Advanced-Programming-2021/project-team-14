<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXProgressBar?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Text?>
<fx:root type="javafx.scene.layout.AnchorPane" xmlns="http://javafx.com/javafx/8.0.171"
         xmlns:fx="http://javafx.com/fxml/1">
    <HBox>
        <children>
            <VBox>
                <children>
                    <HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
                          prefHeight="98.0" prefWidth="240.0">
                        <children>
                            <Circle fx:id="playerPhoto" fill="DODGERBLUE" radius="26.0" stroke="BLACK"
                                    strokeType="INSIDE">
                                <HBox.margin>
                                    <Insets bottom="50.0" left="10.0" top="20.0"/>
                                </HBox.margin>
                            </Circle>
                            <VBox prefHeight="200.0" prefWidth="100.0">
                                <HBox.margin>
                                    <Insets bottom="30.0" left="10.0" top="20.0"/>
                                </HBox.margin>
                                <children>
                                    <Text fx:id="playerNickname" strokeType="OUTSIDE" strokeWidth="0.0">
                                        <VBox.margin>
                                            <Insets top="5.0"/>
                                        </VBox.margin>
                                    </Text>
                                    <Text fx:id="playerLifePoint" layoutX="10.0" layoutY="23.0" strokeType="OUTSIDE"
                                          strokeWidth="0.0">
                                        <VBox.margin>
                                            <Insets top="5.0"/>
                                        </VBox.margin>
                                    </Text>
                                </children>
                            </VBox>
                        </children>
                    </HBox>
                    <JFXProgressBar fx:id="progressBar" prefHeight="12.0" prefWidth="240.0" progress="0.44"/>
                </children>
            </VBox>
        </children>
    </HBox>
</fx:root>
