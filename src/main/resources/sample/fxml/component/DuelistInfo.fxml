<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<?import javafx.scene.layout.*?>

<fx:root stylesheets="@../../CSS/duelistInfo.css" type="javafx.scene.layout.AnchorPane" xmlns="http://javafx.com/javafx/8.0.171"
         xmlns:fx="http://javafx.com/fxml/1">
    <ImageView style="-fx-background-radius: 25; -fx-border-radius: 25;" fx:id="profilePhoto" fitHeight="50.0"
               fitWidth="50.0" layoutX="14.0" layoutY="15.0" pickOnBounds="true" preserveRatio="true"/>
    <Text style="-fx-fill: white;" fx:id="nickname" layoutX="71.0" layoutY="35.0" strokeType="OUTSIDE" strokeWidth="0.0"
          text="Text">
        <font>
            <Font name="System Bold" size="14.0"/>
        </font>
    </Text>
    <Text style="-fx-fill: white;" fx:id="username" layoutX="72.0" layoutY="57.0" strokeType="OUTSIDE" strokeWidth="0.0"
          text="Text">
        <font>
            <Font size="13.0"/>
        </font>
    </Text>
<!--        <JFXProgressBar fx:id="lifePoint" prefWidth="200" layoutY="70"/>-->
</fx:root>
