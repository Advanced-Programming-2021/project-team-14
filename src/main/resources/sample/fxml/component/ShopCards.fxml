<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" prefHeight="82.0" prefWidth="98.0" type="AnchorPane"
         xmlns="http://javafx.com/javafx/8.0.171">
