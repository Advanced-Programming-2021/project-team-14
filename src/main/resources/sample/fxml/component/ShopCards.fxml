                                        style="-fx-background-radius: 20; -fx-border-radius: 50">
            <ImageView fx:id="image" pickOnBounds="true" preserveRatio="true"
                       style="-fx-background-radius: 20; -fx-border-radius: 50"/>
        </javafx.scene.layout.AnchorPane>
        <Circle fx:id="cardFreqCircle" fill="DODGERBLUE" layoutX="98.0" layoutY="13.0" radius="12.0" stroke="BLACK"
                strokeType="INSIDE"/>
