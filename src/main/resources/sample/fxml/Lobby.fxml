<?xml version="1.0" encoding="UTF-8"?>


<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="800" prefWidth="980.0"
            stylesheets="@../CSS/DeckList.css, @../CSS/ScrollPane.css" xmlns="http://javafx.com/javafx/8.0.171"
            fx:controller="graphic.Lobby">
    <children>
        <VBox layoutX="432.0" layoutY="358.0" prefHeight="200.0" prefWidth="126.0">
            <children>
                <JFXButton contentDisplay="CENTER" onAction="#chatRoom" prefHeight="52.0" prefWidth="129.0"
                           stylesheets="@../CSS/LobbyButton.css" text="Chat Room" textFill="#515468">
                    <VBox.margin>
                        <Insets/>
                    </VBox.margin>
                    <font>
                        <Font name="System Bold" size="14.0"/>
                    </font>
                </JFXButton>
                <JFXButton onAction="#sendRequest" prefHeight="61.0" prefWidth="129.0"
                           stylesheets="@../CSS/LobbyButton.css" text="Send Request" textFill="#515468">
                    <VBox.margin>
                        <Insets top="10.0"/>
                    </VBox.margin>
                    <font>
                        <Font name="System Bold" size="14.0"/>
                    </font>
                </JFXButton>
            </children>
        </VBox>
    </children>
</AnchorPane>
